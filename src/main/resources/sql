IF NOT EXISTS (SELECT 1 FROM sysobjects WHERE NAME='nocatuser' and XTYPE='U')
  CREATE TABLE nocatuser (
    id      INT           IDENTITY  PRIMARY KEY,
    deviceId    NVARCHAR(255),
    userName NVARCHAR(255),
    latLong NVARCHAR(255),
  );

